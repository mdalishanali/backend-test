{
  "name": "portfolio-dashboard",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "ng build && tsc --project lib && NODE_ENV=production node server/lib/index.js",
    "tsc-watch": "node verifyEnvBackEnd && tsc --project lib --watch",
    "tsc": "node verifyEnvBackEnd.js && tsc --project lib",
    "server": "nodemon server/lib/index.js",
    "windows-server": "nodemon server/lib/index.js",
    "cron-server": "node server/lib/cron/cron.js",
    "mocha-debug": "NODE_PATH=./ $(npm bin)/mocha",
    "tslint-pre": "sh -c ./scripts/tslint.sh",
    "tslint-fix": "tslint --fix -p .",
    "seed-prod-db": ".\\bash-scripts\\seed-production.sh",
    "seed-staging-db": "ARCHIVE=../export.archive ./bash-scripts/seed-staging.sh",
    "seed-local-from-staging": "./bash-scripts/seed-local-from-staging.sh ../export.archive",
    "seed-local-from-production": ".\\bash-scripts\\seed-local-from-prod.sh ../export.archive",
    "copy-prod-db": ".\\bash-scripts\\copy-prod-db.sh",
    "copy-staging-db": "ARCHIVE=../export.archive ./bash-scripts/copy-staging-db.sh",
    "copy-local-db": ".\\bash-scripts\\copy-local-db.sh",
    "copy-local-db-win": "sh ./bash-scripts/copy-local-db.sh",
    "copy-prod-db-win": "sh ./bash-scripts/copy-prod-db.sh",
    "copy-staging-db-win": "sh ./bash-scripts/copy-staging-db.sh",
    "seed-local-from-staging-win": "sh ./bash-scripts/seed-local-from-staging.sh",
    "seed-local-from-prod-win": "sh ./bash-scripts/seed-local-from-prod.sh",
    "seed-prod-db-win": "sh ./bash-scripts/seed-production.sh",
    "seed-staging-db-win": "sh ./bash-scripts/seed-staging.sh",
    "server-test": "export NODE_ENV=test && node verifyEnvBackEnd.js && tsc --project lib && mocha test --timeout 10000 --exit",
    "server-test-win": "SET NODE_ENV=test&& node verifyEnvBackEnd.js && tsc --project lib && mocha test --timeout 10000 --exit",
    "add-mock-data": "node mock-data-generator.js"
  },
  "engines": {
    "node": "10.16.0"
  },
  "pre-commit": [
    "tslint-pre"
  ],
  "private": true,
  "dependencies": {
    "@nishantsingh/dookie": "0.0.2",
    "@sendgrid/mail": "^7.4.0",
    "@types/express": "^4.11.0",
    "@types/geojson": "^7946.0.10",
    "@types/http-status": "^0.2.30",
    "@types/socket.io": "1.4.38",
    "@types/stripe-v3": "^3.1.23",
    "atob": "^2.1.2",
    "aws-sdk": "^2.814.0",
    "busboy-body-parser": "^0.3.2",
    "casual": "^1.6.2",
    "chai-http": "^4.0.0",
    "compression": "^1.7.4",
    "connect-busboy": "0.0.2",
    "cors": "^2.8.5",
    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "firebase": "^8.10.0",
    "firebase-admin": "^10.0.0",
    "http-status": "^1.0.1",
    "jsonwebtoken": "^8.3.0",
    "jwplatform": "^0.0.6",
    "jwt-simple": "^0.5.1",
    "lodash.debounce": "^4.0.8",
    "mailchimp-api-v3": "^1.12.1",
    "mask-json": "^1.0.3",
    "moment": "^2.29.1",
    "mongoose": "^5.0.2",
    "nock": "^13.2.1",
    "node-cron": "^3.0.0",
    "node-fetch": "^2.6.1",
    "node-geocoder": "^4.2.0",
    "nodemon": "^2.0.12",
    "referral-codes": "^2.0.3",
    "rxjs": "^6.6.0",
    "rxjs-compat": "^6.6.7",
    "standard-error": "^1.1.0",
    "stripe": "^8.106.0",
    "sweetalert2": "^7.20.7",
    "tslib": "^2.0.0",
    "twilio": "^3.17.0",
    "validator": "^13.6.0",
    "winston": "^3.6.0",
    "winston-aws-cloudwatch": "^3.0.0",
    "yaml": "^1.10.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.0",
    "@types/node": "^12.19.9",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "mocha": "^9.2.1",
    "pre-commit": "1.2.2",
    "ts-node": "^6.0.3",
    "tslint": "~6.1.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "~4.3.5"
  },
  "browser": {
    "crypto": false
  }
